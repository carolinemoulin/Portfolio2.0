<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <meta name="description" content="Portfolio de sites web">
        <meta name="author" content="Caroline Moulin">
        <link rel="icon" href="{{ asset('images/favicon.ico') }}" />
        <meta property="og:image" content="https://caroline-moulin.fr/public/img/" /> <!-- TO DO -->
        <meta property="og:url" content="https://caroline-moulin.fr/" />
        <meta property="og:type" content="website" />
        <meta property="og:title" content="Portfolio de Caroline Moulin" />
        <meta property="og:description" content="Portfolio de sites web" />
        <!--Vcard twitter-->
        <meta name="twitter:card" content="summary" />
        <meta name="twitter:site" content="Portfolio de Caroline Moulin" />
        <meta name="twitter:creator" content="Caroline Moulin" />

        <title>{% block title %}Portfolio de Caroline MOULIN{% endblock %}</title>

        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
            <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,700" rel="stylesheet">

            <link href="{{ asset('build/beckham/css/open-iconic-bootstrap.min.css') }}" type="text/css" rel="stylesheet">
            <link href="{{ asset('build/beckham/css/animate.css') }}" type="text/css" rel="stylesheet">

            <link href="{{ asset('build/beckham/css/owl.carousel.min.css') }}" type="text/css" rel="stylesheet">
            <link href="{{ asset('build/beckham/css/owl.theme.default.min.css') }}" type="text/css" rel="stylesheet">
            <link href="{{ asset('build/beckham/css/magnific-popup.css') }}" type="text/css" rel="stylesheet">

            <link href="{{ asset('build/beckham/css/aos.css') }}" type="text/css" rel="stylesheet">

            <link href="{{ asset('build/beckham/css/ionicons.min.css') }}" type="text/css" rel="stylesheet">

            <link href="{{ asset('build/beckham/css/bootstrap-datepicker.css') }}" type="text/css" rel="stylesheet">
            <link href="{{ asset('build/beckham/css/jquery.timepicker.css') }}" type="text/css" rel="stylesheet">

            <link href="{{ asset('build/beckham/css/flaticon.css') }}" type="text/css" rel="stylesheet">
            <link href="{{ asset('build/beckham/css/icomoon.css') }}" type="text/css" rel="stylesheet">
            <link href="{{ asset('build/beckham/css/style.css') }}" type="text/css" rel="stylesheet">

            <link rel="stylesheet" href="https://unpkg.com/leaflet@1.3.1/dist/leaflet.css" />
            <link rel="stylesheet" href="https://unpkg.com/leaflet@1.3.1/dist/leaflet.css" />

            <!-- Custom styles for this template -->
            <link href="{{ asset('css/stylePerso.css') }}" type="text/css" rel="stylesheet"/>

        {% endblock %}
    </head>
    <body>
        <!-- Navigation -->
        <div class="KW_progressContainer">
                <div class="KW_progressBar">

                </div>
        </div>
            <div class="page">
                <nav id="colorlib-main-nav" role="navigation">
                    <a href="#" class="js-colorlib-nav-toggle colorlib-nav-toggle active"><i></i></a>
                    <div class="js-fullheight colorlib-table">
                        <div class="img" style="background-image: url({{ asset('images/profile.JPG') }});"></div>
                        <div class="colorlib-table-cell js-fullheight">
                            <div class="row no-gutters">
                                <div class="col-md-12 text-center">
                                    <h1 class="mb-4"><a href="{{path('home')}}" class="logo">Caroline Moulin</a></h1>
                                    <ul>
                                        <li class="active"><a href="{{path('home')}}"><span><small>01</small>Accueil</span></a></li>
                                        <li><a href="{{path('about')}}"><span><small>02</small>A propos</span></a></li>
                                        <li><a href="{{path('portfolio')}}"><span><small>03</small>Portfolio</span></a></li>
                                        <li><a href="{{path('contact')}}"><span><small>04</small>Contact</span></a></li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </nav>

                <div id="colorlib-page">
                    <header>
                        <div class="container">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="colorlib-navbar-brand">
                                        <a class="colorlib-logo" href="{{path('home')}}"><span class="logo-img" style="background-image: url({{ asset('images/profile.JPG') }});"></span>Caroline Moulin</a>
                                    </div>
                                    <a href="#" class="js-colorlib-nav-toggle colorlib-nav-toggle"><i></i></a>
                                </div>
                            </div>
                        </div>
                    </header>


        {% block body %}{% endblock %}
                </div>
                {{ include ('accueil/include/footer.html.twig') }}
            </div>
        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
            <script src="{{ asset('build/beckham/js/jquery.min.js') }}"></script>
            <script src="{{ asset('build/beckham/js/jquery-migrate-3.0.1.min.js') }}"></script>
            <script src="{{ asset('build/beckham/js/popper.min.js') }}"></script>
            <script src="{{ asset('build/beckham/js/bootstrap.min.js') }}"></script>
            <script src="{{ asset('build/beckham/js/jquery.easing.1.3.js') }}"></script>
            <script src="{{ asset('build/beckham/js/jquery.waypoints.min.js') }}"></script>
            <script src="{{ asset('build/beckham/js/jquery.stellar.min.js') }}"></script>
            <script src="{{ asset('build/beckham/js/owl.carousel.min.js') }}"></script>
            <script src="{{ asset('build/beckham/js/jquery.magnific-popup.min.js') }}"></script>
            <script src="{{ asset('build/beckham/js/aos.js') }}"></script>
            <script src="{{ asset('build/beckham/js/jquery.animateNumber.min.js') }}"></script>
            <script src="{{ asset('build/beckham/js/scrollax.min.js') }}"></script>
            <script src="{{ asset('build/beckham/js/bootstrap-datepicker.js') }}"></script>
            <script src="{{ asset('build/beckham/js/jquery.timepicker.min.js') }}"></script>
            <script src="{{ asset('build/beckham/js/main.js') }}"></script>
            <script src="https://unpkg.com/leaflet@1.3.1/dist/leaflet.js"></script>
            <script src="{{ asset('js/map.js') }}"></script>


        {% endblock %}
    </body>
</html>
